g in else condition</h1>\n        <button v-on:click=\"display\">toggle</button>\n        <table border=\"1px\">\n            <tr>\n                <th>Id</th>\n                <th>Name</th>\n                <th>Email</th>\n            </tr>\n            <tr v-for=\"item in users\" :key=\"item.id\">\n                <td>{{item.id}}</td>\n                <td>{{item.name}}</td>\n                <td>{{item.email}}</td>\n            </tr>\n        </table>\n        <Child1/>\n    </div>\n</template>\n\n<script>\nimport Child1 from './Child1.vue';\nexport default {\n  components: { Child1 },\n    name : \"Home\",\n    // props : {\n    //     msg : String\n    // },\n     props : [\n         'msg'\n     ],\n    methods : {\n        hello(data){\n            console.log(\"hello function called\");\n            alert(data)\n        },\n        display(){\n            this.show=!this.show\n        }\n    },\n    data(){\n        return {\n            show:true,\n            users : [\n                {id:1,name:\"paul\",email:\"paul@gmail.com\"},\n                {id:2,name:\"sid\",email:\"sid@gmail.com\"},\n                {id:3,name:\"max\",email:\"max@gmail.com\"}\n            ]\n        }\n    },\n}\n</script>"]}]}